package com.Ejercicio4;

public class SmartDeviceMain {

    public static void main(String[] args) {

        // Creamos los objetos

        SmartPhone Telefono1 = new SmartPhone("Telefono","Rojo","Apple","Iphone 12",50000,false);
        SmartPhone Telefono2 = new SmartPhone("Telefono","Blanco","Apple","Iphone 12 Pro",5400,false);

        SmartWatch Reloj1 = new SmartWatch("Reloj","Blanco","Apple","Serie 2",2000,false,"Tela",4.5f);
        SmartWatch Reloj2 = new SmartWatch("Reloj","Negro","Samsung","Gear",2000,false,"Metalica",4.5f);

        // Imprimimos sus valores por consola

        System.out.println(Telefono1);
        System.out.println(Telefono2);
        System.out.println(Reloj1);
        System.out.println(Reloj2);
    }
}
