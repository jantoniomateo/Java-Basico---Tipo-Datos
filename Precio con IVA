public class FuncionIva {

    public static void main(String[] args) {

        //Este programa calcula el precio con iva incluido de 120 al 21%

        int cantidadSinIva = 120;
        float iva = 0.21f;

        float precioConIva = calculoIva (cantidadSinIva, iva);

        System.out.println("El importe sin iva es: "+ cantidadSinIva);
        System.out.println("El iva aplicado es del: "+ iva);
        System.out.println("El importe con iva es: "+ precioConIva);

    }

    public static float calculoIva (int cantidadSinIva, float iva){
        return  cantidadSinIva * iva;


    }
}
